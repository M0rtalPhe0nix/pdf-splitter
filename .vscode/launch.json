    "version": "0.2.0",
    "configurations": [
    {
        "type": "cmake",
        "request": "launch",
        "name": "CMake: Externally launched",
        "cmakeDebugType": "external",
        "pipeName": "<...>"
    },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "pdf_splitter/bin/div_pdf.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["Thinking, Fast and Slow (Daniel Kahneman).pdf"],
            "cwd": "${workspaceFolder}"

        }
    ]
}